server {
	host 127.0.0.1;
	listen 8080;
	server_name server1;
	root  application/;
	index src/index.html;
	body_size 8192;

	error_page		404 error_pages/404.html;
	methods 		GET     POST    DELETE;

	location /test {
        root data/www/test;
        index index.html;
        autoindex ON;
    }
}