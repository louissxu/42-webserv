server {
	listen 127.0.0.1:8080;
	listen 8081;
	server_name server1;
	root  data/www;
	index index.html;
	body_size 8192;

	error_page		404 error_pages/404.html;
	methods 		GET     POST    DELETE;

	location /test {
        root data/www/test;
        index index.html;
        autoindex ON;
    }
}
